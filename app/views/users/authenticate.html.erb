<div class="form">
  <%= render 'flash_messages'%>
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">Log In</h3>
    </div>
    <div class="panel-body">
      <%= bootstrap_form_tag url: auth_path do |f| %>
        <%= f.email_field :email %>
        <%= f.password_field :password %>
        <%= f.submit 'Log In' %>
      <% end %>
    </div>
  </div>
</div>
